<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.boranews">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_boranews_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BoraNews.NoActionBar">
        <activity android:name=".ui.internationallist.InternationalNewsActivity"></activity>
        <activity
            android:name=".SearchActivity"
            android:theme="@style/Theme.BoraNews"
            android:label="@string/search">
            >
        </activity>
        <activity android:name=".DetailNewsActivity" />
        <activity android:name=".WebViewNewsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>